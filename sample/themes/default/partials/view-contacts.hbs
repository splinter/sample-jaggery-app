<div>
	
<table class='table'>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Telephone</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
			<!-- 
			The each helper is a default Handlebars helper which allows an array
			of elements to iterated 
			-->
			<tr>
			{{#each contacts}}
				<td>{{this.fName}}</td>
				<td>{{this.lName}}</td>
				<td>{{this.telephone}}</td>
				<td>{{this.email}}</td>
			{{/each}}
			</tr>
	</tbody>
</table>>

</div>